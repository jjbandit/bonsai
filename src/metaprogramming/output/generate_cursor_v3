
struct v3_cursor
{
  v3* Start;
  v3* End;
  v3* At;
};

bonsai_function v3_cursor
V3Cursor(umm ElementCount, memory_arena* Memory)
{
  // TODO(Jesse): Can we use Allocate() here instead?
  v3* Start = (v3*)PushStruct(Memory, sizeof(v3), 1, 1);
  v3_cursor Result = {
    .Start = Start,
    .End = Start+ElementCount,
    .At = Start,
  };
  return Result;
};

